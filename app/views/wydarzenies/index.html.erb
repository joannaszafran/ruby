<h1>Listing wydarzenies</h1>

<table>
  <tr>
    <th>Nazwa</th>
    <th>Opis</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @wydarzenies.each do |wydarzeny| %>
  <tr>
    <td><%= wydarzeny.nazwa %></td>
    <td><%= wydarzeny.opis %></td>
    <td><%= link_to 'Show', wydarzeny %></td>
    <td><%= link_to 'Edit', edit_wydarzeny_path(wydarzeny) %></td>
    <td><%= link_to 'Destroy', wydarzeny, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

